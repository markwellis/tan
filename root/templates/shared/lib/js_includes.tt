[%- js_inc_done = {} -%]
[% FOREACH js_include IN js_includes -%]
    [% IF !js_inc_done.$js_include %]
        [% IF js_include.match('^(?:http|/static)') %]
            <script src="[% js_include %]" async></script>
        [% ELSE %]
            <script src="[% c.config.cache_path %]/minify/[% c.VERSION %]/[% theme_settings.name %]/js/[% js_include %].js" async></script>
        [% END %]
        [%- js_inc_done.$js_include = 1 -%]
    [% END %]
[% END %]
